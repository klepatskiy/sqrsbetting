{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-sockets": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.14",
        "fresh/centrifugo-bundle": "*",
        "harmbandstra/swagger-ui-bundle": "^5.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.20",
        "roadrunner-php/centrifugo": "*",
        "runtime/roadrunner-symfony-nyholm": "^0.3.1",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^6.4",
        "symfony/property-access": "^6.4",
        "symfony/property-info": "^6.4",
        "symfony/psr-http-message-bridge": "^6.4",
        "symfony/redis-messenger": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/uid": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "rector/rector": "^0.15.25",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "symplify/config-transformer": "^12.0",
        "symplify/easy-coding-standard": "^11.3",
        "vimeo/psalm": "^5.10"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "OasModels\\": "external/openapigen/models/lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "symfony-scripts": [
            "HarmBandstra\\SwaggerUiBundle\\Composer\\ScriptHandler::linkAssets"
        ],
        "post-install-cmd": [
            "@auto-scripts",
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@symfony-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4"
        }
    }
}
